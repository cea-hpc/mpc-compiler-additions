+bool
+mpc_decl_match_invalid_header(tree decl)
+{
+	bool invalid = 0;
+
+  if(DECL_IN_SYSTEM_HEADER(decl))
+  {
+    /* In some cases when using macros from headers to construct
+    object a local variable may appear as being from header
+    we then disqualify the case where we are in the current input file
+
+    For example a global such as:
+      int privatize(syscall(SYS_gettid));
+     */
+
+    /* No info assume from header */
+    if (!DECL_SOURCE_FILE(decl) || !main_input_filename)
+      return 1;
+
+    if (strcmp(DECL_SOURCE_FILE(decl), main_input_filename))
+    {
+      /* Is is actually from header and not local macro exp */
+      return 1;
+    }
+
+  }
+	const char * name = lang_hooks.decl_printable_name (decl,0);
+
+
+	if(invalid_headers == NULL)
+	{
+		invalid_headers = mpc_decl_load_invalid_from_getenv("AP_UNPRIVATIZED_FILES");
+	}
+	
+	if(invalid_vars == NULL)
+	{
+		invalid_vars = mpc_decl_load_invalid_from_getenv("AP_UNPRIVATIZED_VARS");
+	}
+
+	int i = 0;
+	for (i=0; i < DEFAULT_NB_CELLS; i++)
+	{
+		if(invalid_headers &&  *(invalid_headers + i) != NULL)
+		{
+			if(strcmp(basename(DECL_SOURCE_FILE(decl)), invalid_headers[i]) == 0)
+			{
+        invalid = 1;
+				break;
+			}
+		}
+		
+		if(invalid_vars && *(invalid_vars + i) != NULL)
+		{
+			if(strcmp(name, invalid_vars[i]) == 0)
+			{
+        /*fprintf(stderr, "Matching %s w/ %s\n", name, invalid_vars[i]);*/
+				invalid = 1;
+				break;
+			}
+		}
+	}
+
+	return invalid;
