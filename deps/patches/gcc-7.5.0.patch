+		match ( "node", gfc_match_hls_node, ST_HLS_SCOPE);
+		match ( "numa", gfc_match_hls_numa, ST_HLS_SCOPE);
+		break;
+	case 's':
+		match ( "socket", gfc_match_hls_socket, ST_HLS_SCOPE);
+		match ( "single", gfc_match_hls_single, ST_HLS_SINGLE);
+		break;
