+  /* skip level clause if found */
+  if ( level_clause_found == 1 ) 
+	  gfc_current_locus = loc_after_level ;
+  return MATCH_YES;
+
+syntax:
+  gfc_error ("Syntax error in !$HLS list at %C");
+
+cleanup:
+  gfc_current_locus = old_loc;
+  return MATCH_ERROR;
+}
